/* See target-hooks-macros.h for details of macros that should be
   provided by the including file, and how to use them here.  */

#include "target-hooks-macros.h"

#undef HOOK_TYPE
#define HOOK_TYPE "D Target Hook"

HOOK_VECTOR (TARGETDM_INITIALIZER, gcc_targetdm)

#undef HOOK_PREFIX
#define HOOK_PREFIX "TARGET_"

/* Environmental version identifiers relating to the target OS.  */
DEFHOOK
(d_os_builtins,
 "Declare predefined version identifiers",
 void, (void),
 hook_void_void)

/* Environmental version identifiers relating to the target CPU.  */
DEFHOOK
(d_cpu_builtins,
 "Declare predefined version identifiers",
 void, (void),
 hook_void_void)

/* The size of OS CRITICAL_SECTION or pthread_mutex_t.  */
DEFHOOK
(d_critsec_size,
 "Declare size of critical section",
 unsigned, (void),
 hook_uint_void_0)

/* Close the 'struct gcc_targetdm' definition.  */
HOOK_VECTOR_END (C90_EMPTY_HACK)
